{
  "name": "@tixlfinance/tixlfinance-db",
  "version": "1.3.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "generate:token-schema": "npx typescript-json-validator scripts/schema.ts TokenInformationSchema",
    "validate": "ts-node scripts/validate.ts",
    "lint": "tslint scripts/**/*.ts{,x}",
    "lint:fix": "tslint scripts/**/*.ts{,x} --fix",
    "prettier": "prettier --write scripts/**/*.ts",
    "push:projects": "ts-node scripts/pushProjects.ts",
    "push:exchanges": "ts-node scripts/pushExchanges.ts",
    "scores:calculate": "ts-node scripts/calcScoresAndGenerateCsv.ts",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tixlfinance/tixlfinance-db.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run prettier"
    }
  },
  "dependencies": {
    "cross-fetch": "^3.0.6",
    "dotenv": "^8.2.0",
    "graphql": "^15.3.0",
    "graphql-request": "^3.1.0",
    "typescript-json-validator": "^2.4.2"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.1",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "prettier": "^2.1.2",
    "ts-jest": "^26.4.0",
    "ts-node-dev": "^1.0.0-pre.62",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.3"
  },
  "author": "",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/tixlfinance/tixlfinance-db/issues"
  },
  "homepage": "https://github.com/tixlfinance/tixlfinance-db#readme",
  "description": "## Contribution Quick Start"
}
